{
  "mcpServers": {
    "exai-mcp": {
      "command": "python",
      "args": [
        "-u",
        "scripts/runtime/run_ws_shim.py"
      ],
      "env": {
        "ENV_FILE": ".env",
        "PYTHONUNBUFFERED": "1",
        "PYTHONIOENCODING": "utf-8"
      }
    },
    "minimax_search": {
      "description": "MiniMax Search - Web search and intelligent browsing",
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/MiniMax-AI/minimax_search",
        "minimax-search"
      ],
      "env": {
        "MINIMAX_API_KEY": "${MINIAGENT_API_KEY}"
      }
    }
  }
}