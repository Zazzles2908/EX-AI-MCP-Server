      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "mermaid-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@narasimhaponnada/mermaid-mcp-server"
      ]
    }
  }
